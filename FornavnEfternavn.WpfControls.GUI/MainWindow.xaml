<Window x:Class="FornavnEfternavn.WpfControls.GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FornavnEfternavn.WpfControls.GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1000">
    <Grid>

        <!--<local:MovieListControl />-->

        <local:PageHost CurrentPage="{Binding ApplicationViewModel.CurrentPage, Source={x:Static local:ViewModelLocator.Instance}}"
